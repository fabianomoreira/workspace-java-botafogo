package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cao = new Cachorro();
		
		cao.brincar();
		cao.emitirSom();
		
		gato.setCor("Branco");
		gato.setRaca("Vira lata");
		gato.setPeso(-2);
		gato.saltar();
		gato.emitirSom();
	}

}
