package zoo;

public class Gato {
	String cor;
	String raca;
	double peso;
	
	public void emitirSom() {
		System.out.println("Miau!");
		System.out.println("Miaaau!");
		System.out.println("Miau! Miau!");
	}
	
	public void brincar() {
		System.out.println("brincando...");
	}
	
	public void saltar() {
		System.out.println("saltando...");
	}
}
